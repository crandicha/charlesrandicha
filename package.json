{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "img:resize:company": "rm -rf public/images/company/100 && mkdir public/images/company/100 && yarn sharp -i public/images/company/*.{svg,png} -o public/images/company/100/{base} resize 100 100",
    "img:resize:contact": "rm -rf public/images/contact/32 && mkdir public/images/contact/32 && yarn sharp -i public/images/contact/*.{svg,png} -o public/images/contact/32/{base} resize 32 32",
    "img:resize:all": "yarn img:resize:company && yarn img:resize:contact",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/image": "^0.15.1",
    "@astrojs/mdx": "^0.15.1",
    "@astrojs/react": "^2.0.1",
    "@astrojs/tailwind": "^3.0.0",
    "@fontsource/inter": "^4.5.15",
    "@fontsource/maven-pro": "^4.5.13",
    "@fontsource/montserrat": "^4.5.14",
    "@fontsource/poppins": "^4.5.10",
    "@material-ui/core": "^4.12.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^2.0.15",
    "clsx": "^1.2.1",
    "framer-motion": "^10.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-responsive": "^9.0.2",
    "react-toastify": "^9.1.1",
    "sharp": "^0.31.3",
    "sharp-cli": "^4.1.0",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-plugin-astro": "^0.23.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "prettier": "^2.8.3",
    "prettier-plugin-astro": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.2.2"
  }
}
